{
  "name": "beardify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "lint": "vue-tsc --noEmit && prettier --c ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss}",
    "build": "yarn lint && vite build",
    "fix": "prettier --write ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss} --fix",
    "preview": "yarn build && yarn vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^6.5.3",
    "axios": "^0.22.0",
    "form-urlencoded": "^6.0.4",
    "pinia": "^2.0.0-rc.10",
    "pkce": "^1.0.0-beta2",
    "vue": "^3.2.20",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.5",
    "vue-tsc": "^0.28.1"
  }
}
