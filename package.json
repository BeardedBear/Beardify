{
  "name": "beardify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "lint": "vue-tsc --noEmit && eslint src/**/*.{vue,ts} && prettier --c ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss}",
    "build": "yarn lint && vite build",
    "fix": "prettier --write ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss} --fix",
    "preview": "yarn build && yarn vite preview",
    "lint:commit": "vue-tsc --noEmit",
    "pre-commit": "yarn lint:commit"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.6",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "form-urlencoded": "^6.0.6",
    "normalize-diacritics": "^3.0.8",
    "pinia": "^2.0.13",
    "pinia-plugin-persistedstate": "^1.5.1",
    "pkce": "^1.0.0-beta2",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vue-slicksort": "^2.0.0-alpha.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.12",
    "postcss-html": "^1.4.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "stylelint": "^14.6.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.6"
  }
}
