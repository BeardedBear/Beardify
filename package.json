{
  "name": "beardify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "lint": "vue-tsc --noEmit && eslint src/**/*.{vue,ts} && prettier --c ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss}",
    "build": "yarn lint && vite build",
    "fix": "prettier --write ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss} --fix",
    "preview": "yarn build && yarn vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "axios": "^0.24.0",
    "date-fns": "^2.28.0",
    "form-urlencoded": "^6.0.5",
    "pinia": "^2.0.9",
    "pkce": "^1.0.0-beta2",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vue-slicksort": "^2.0.0-alpha.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "sass": "^1.46.0",
    "stylelint": "^14.1.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vue-tsc": "^0.30.2"
  }
}
