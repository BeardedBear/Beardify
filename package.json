{
  "name": "beardify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "lint": "vue-tsc --noEmit && eslint src/**/*.{vue,ts} && prettier --c ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss}",
    "build": "yarn lint && vite build",
    "fix": "prettier --write ./src/**/*.{ts,vue,json} && stylelint ./src/**/*.{vue,scss} --fix",
    "preview": "yarn build && yarn vite preview",
    "lint:commit": "vue-tsc --noEmit",
    "pre-commit": "yarn lint:commit"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.0",
    "axios": "^1.4.0",
    "date-fns": "^2.29.3",
    "form-urlencoded": "^6.1.0",
    "normalize-diacritics": "^4.0.0",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "pkce": "^1.0.0-beta2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-slicksort": "^2.0.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.1"
  }
}
